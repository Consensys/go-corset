(defpurefun ((vanishes! :𝔽@loob) x) x)

(defcolumns (X :𝔽) (Y :𝔽))
(defconstraint c1 () (vanishes! (+ X (* 2 Y))))
(defconstraint c2 () (vanishes! (+ (* 2 Y) X)))
(defconstraint c3 () (vanishes! (+ X Y Y)))
(defconstraint c4 () (vanishes! (+ Y X Y)))
(defconstraint c5 () (vanishes! (+ Y Y X)))
(defconstraint c6 () (vanishes! (+ (* 1 X) Y Y)))
(defconstraint c7 () (vanishes! (+ Y (* 1 X) Y)))
(defconstraint c8 () (vanishes! (+ Y Y (* 1 X))))
