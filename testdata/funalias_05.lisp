(defpurefun ((vanishes! :𝔽@loob) x) x)
(defunalias or *)

(defcolumns (X :i16) (Y :i16))
(defconstraint c1 () (vanishes! (or X (* 2 Y))))
(defconstraint c2 () (vanishes! (or (* 2 Y) X)))
(defconstraint c3 () (vanishes! (or X Y Y)))
(defconstraint c4 () (vanishes! (or Y X Y)))
(defconstraint c5 () (vanishes! (or Y Y X)))
(defconstraint c6 () (vanishes! (or (* 1 X) Y Y)))
(defconstraint c7 () (vanishes! (or Y (* 1 X) Y)))
(defconstraint c8 () (vanishes! (or Y Y (* 1 X))))
