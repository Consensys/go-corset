(defcolumns (ST :binary) (X :i16@loob) (Y :i16@loob))

(defconstraint c1 (:guard ST) (- 16 (reduce + (begin X (* 2 Y)))))
(defconstraint c2 (:guard ST) (- 16 (reduce + (begin (* 2 Y) X))))
(defconstraint c3 (:guard ST) (- 16 (reduce + (begin X Y Y))))
(defconstraint c4 (:guard ST) (- 16 (reduce + (begin Y X Y))))
(defconstraint c5 (:guard ST) (- 16 (reduce + (begin Y Y X))))
(defconstraint c6 (:guard ST) (- 16 (reduce + (begin (* 1 X) Y Y))))
(defconstraint c7 (:guard ST) (- 16 (reduce + (begin Y (* 1 X) Y))))
(defconstraint c8 (:guard ST) (- 16 (reduce + (begin Y Y (* 1 X)))))
