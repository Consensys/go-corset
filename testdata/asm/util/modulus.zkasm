;; compute a mod b where a is a u512 and a u256
fn modulus_u512_u256(a u512, b u256) -> (res u256) {
  var quot u512
  var sum u768
  var witsum u769
  var witness u256
  ;; handle division by 0
  if b == 0 goto exit_trivial
  ;;
  quot, res, witness, sum, witsum = a / b
  ;;
  return
exit_trivial:
var tmp u256
  tmp, res = a
  return
}
