include "max.zkasm"

fn diff(x u256, y u256) -> (res u256) {
  var m u256
  var n u256
  var c u1
  ;;
  m,n = minmax(x,y)
  ;;
  c, res = n - m
  ;;
  return
}

fn minmax(x u256, y u256) -> (min u256, max u256) {
  min = min256(x,y)
  max = max256(x,y)
  return
}

fn min256(arg1 u256, arg2 u256) -> (res u256) {
   var b u1
   var tmp u256
   b,tmp = arg1 - arg2
   if b==0 goto exit1
   res = arg1
   return
exit1:
   res = arg2
   return
}
