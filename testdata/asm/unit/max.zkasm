fn max256(arg1 u256, arg2 u256) -> (res u256) {
   var b u1
   var tmp u256
   b,tmp = arg1 - arg2
   if b!=0 goto exit1
   res = arg1
   return
exit1:
   res = arg2
   return
}

fn max16(x u16, y u16) -> (z u16) {
   var b u1
   var tmp u16
   b,tmp = x - y
   if b!=0 goto exit1
   z = x
   return
exit1:
   z = y
   return
}

