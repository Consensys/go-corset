;; OR module - to be used in BIN

include "common_bin.zkasm"

const EVM_INST_OR = 0x17

fn or(ARG_1 u256, ARG_2 u256) -> (RES u256) {
    ;; RES = intermediate_u256(EVM_INST_OR, ARG_1, ARG_2) => gives me an unknown register error
    var inst u8
    inst = 0x17
    RES = intermediate_u256(inst, ARG_1, ARG_2)
    return
}

fn or_u1(ARG_1 u1, ARG_2 u1) -> (RES u1) {
  var c0, c1 u1
  c0, c1, RES = ( ARG_1 + ARG_2 ) - ( ARG_1 * ARG_2 )
  return
}