fn div(x u16, y=1 u16) -> (q u16, r u16) {
  var sum1, sum2 u32
  var _q, _r, wit u16
  var witsum1, witsum2 u17
  ;;
  q, r, wit, sum1, witsum1 = x / y
  ;; create shadows to force write conflict on wit.
  _q, _r, wit, sum2, witsum2 = x / y
  ;; check shadows
  if q != _q goto exit_f
  if r != _r goto exit_f
  ;;
  return
exit_f:
  fail
}
