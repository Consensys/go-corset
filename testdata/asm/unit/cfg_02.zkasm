;; Illustrates a program where after a merge point we have ambiguity
;; regarding whether to read v or v'.
fn f(x u32, y u32) -> (res u1){
  var i u32
  var c, f u1
  ;;
  i = 0
  f = 0
  ;;
loop:
  c, i = i + 1
  if i != y goto cont
  f = 1
cont:
  if i != x goto exit
  goto loop
exit:
  res = f
  return
}
