;; LT                               0x10
;; GT                               0x11
;; SLT                              0x12
;; SGT                              0x13
;; EQ                               0x14
;; ISZERO                           0x15
fn wcp(inst u8, arg1 u32, arg2 u32) -> (res u1) {
   var tmp u8
   var targ u32
   var b u1
   if inst==16 goto lt
   if inst==17 goto gt
   if inst==20 goto eq
   goto isz
lt:
   b,targ = arg1 - arg2
   if b!=0 goto exit_1
   goto exit_0
gt:
   b,targ = arg2 - arg1
   if b!=0 goto exit_1
   goto exit_0
eq:
   if arg1==arg2 goto exit_1
   goto exit_0
isz:
   if arg1==0 goto exit_1
exit_0:
   res = 0
   return
exit_1:
   res = 1
   return
}
