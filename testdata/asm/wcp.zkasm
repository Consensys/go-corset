;; LT                               0x10
;; GT                               0x11
;; SLT                              0x12
;; SGT                              0x13
;; EQ                               0x14
;; ISZERO                           0x15
fn wcp(inst u8, arg1 u32, arg2 u32) -> (res u1) {
   var tmp u8
   var targ u32
   var b u1
   jc inst==16 lt
   jc inst==17 gt
   jc inst==20 eq
   jmp isz
lt:
   b,targ = arg1 - arg2
   jc b!=0 exit_1
   jmp exit_0
gt:
   b,targ = arg2 - arg1
   jc b!=0 exit_1
   jmp exit_0
eq:
   jc arg1==arg2 exit_1
   jmp exit_0
isz:
   jc arg1==0 exit_1
exit_0:
   res = 0
   ret
exit_1:
   res = 1
   ret
}
