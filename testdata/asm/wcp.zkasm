;; LT                               0x10
;; GT                               0x11
;; SLT                              0x12
;; SGT                              0x13
;; EQ                               0x14
;; ISZERO                           0x15
fn wcp(inst u8, arg1 u32, arg2 u32) -> (res u1) {
   var tmp u8
   var targ u32
   var b u1
   ;; inst == EVM_INST_LT
   b,tmp = inst - 16
   jz tmp lt
   ;; inst == EVM_INST_GT
   b,tmp = inst - 17
   jz tmp gt
   ;; inst == EVM_INST_EQ
   b,tmp = inst - 20
   jz tmp eq
   ;; else
   jmp isz
lt:
   b,targ = arg1 - arg2
   jnz b exit_1
   jmp exit_0
gt:
   b,targ = arg2 - arg1
   jnz b exit_1
   jmp exit_0
eq:
   b,targ = arg1 - arg2
   jnz b exit_0
   jnz targ exit_0
   jmp exit_1
isz:
   jz arg1 exit_1
exit_0:
   res = 0
   jmp exit
exit_1:
   res = 1
exit:
   ret
}
