(defcolumns (X :i32) (Y :i32))
(defconstraint c1 () (== 0 (+ X (* -2 Y))))
(defconstraint c2 () (== 0 (+ (* -2 Y) X)))
(defconstraint c3 () (== 0 (+ X (* -1 Y) (* -1 Y))))
(defconstraint c4 () (== 0 (+ (* -1 Y) X (* -1 Y))))
(defconstraint c5 () (== 0 (+ (* -1 Y) (* -1 Y) X)))
(defconstraint c6 () (== 0 (+ (* 1 X) (* -1 Y) (* -1 Y))))
(defconstraint c7 () (== 0 (+ (* -1 Y) (* 1 X) (* -1 Y))))
(defconstraint c8 () (== 0 (+ (* -1 Y) (* -1 Y) (* 1 X))))
